{
    "name": "angular-component-file-cycler",
    "displayName": "Angular Component File Cycler",
    "description": "Press Ctrl+K, Ctrl+O to cycle through the ts/html/scss files of a single Angular component.",
    "version": "1.0.5",
    "publisher": "fordesoft",
    "engines": {
        "vscode": "^1.19.0"
    },
    "icon": "images/icon.png",
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.cycleAngularComponentFiles",
        "onCommand:extension.cycleAngularComponentFilesReverse"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.cycleAngularComponentFiles",
                "title": "Cycle between Angular component files"
            },
            {
                "command": "extension.cycleAngularComponentFilesReverse",
                "title": "Reverse cycle between Angular component files"
            }
        ],
        "keybindings": [
            {
                "command": "extension.cycleAngularComponentFiles",
                "key": "ctrl+k ctrl+o",
                "when": "editorTextFocus"
            },
            {
                "command": "extension.cycleAngularComponentFiles",
                "key": "command+k command+o",
                "when": "editorTextFocus"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test",
        "publish": "vsce publish"
    },
    "devDependencies": {
        "@types/mocha": "^2.2.42",
        "@types/node": "^17.0.34",
        "typescript": "^4.6.4",
        "vscode": "^1.1.6"
    }
}
